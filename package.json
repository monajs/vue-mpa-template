{
  "name": "@bbt.pandora/scaffold-vue-mpa",
  "version": "3.5.0",
  "description": "vue mpa projectp",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=dev && npm run config && node ./build/webpack.dev.js",
    "build:sit": "export NODE_ENV=sit && npm run build:common",
    "build:pre": "export NODE_ENV=pre && npm run build:common",
    "build:production": "export NODE_ENV=production && npm run build:common",
    "build:common": "npm run config && node ./build/webpack.build.js",
    "config": "node ./build/scripts/genConfig.js",
    "proxy": "node ./build/scripts/proxy.js",
    "lint": "eslint --fix --ext .js,.vue src && stylelint ./src/style/*.less ./src/views/**/*.less --fix"
  },
  "mtConfig": {
    "framework": "vue",
    "type": "mpa"
  },
  "keywords": [
    "vue-mpa",
    "scaffold"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^2.0.2",
    "console.table": "^0.10.0",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^1.0.0",
    "deep-extend": "^0.6.0",
    "eslint": "^5.6.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-vue": "^5.2.2",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.1.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "loader-core": "^0.1.0",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.0",
    "postcss-less": "^3.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-px2rem": "^0.3.0",
    "postcss-px2rem-exclude": "^0.0.6",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "vue-hot-reload-api": "^2.3.3",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@bbt/mt-utils-tracking": "^1.1.22",
    "@monajs/events": "^1.1.1",
    "@monajs/position": "^1.1.1",
